rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /class-dict/{document=**} {
      allow read: if true;
      allow write: if get(/databases/$(database)/documents/user-details/$(request.auth.uid)).admin == true;
    }
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}